call pathogen#infect()

set nocompatible
set autoindent
filetype plugin indent on
set ofu=syntaxcomplete
set t_Co=256
set number
syntax enable
set background=dark
set diffopt+=vertical
set tabstop=4
set shiftwidth=4
set noexpandtab
colorscheme zenburn
if has('gui_running')
  set guifont=Source\ Code\ Pro\ Light:h11
endif
